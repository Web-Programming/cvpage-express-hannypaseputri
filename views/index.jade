extends layout

block content
  header.d-print-none
      div.container.text-center.text-lg-left
        div.py-3.clearfix
          h1.site-title.mb-0 Hanny Pase Putri
          div.site-nav
            nav(role='navigation')
              ul.nav.justify-content-center
                li.nav-item
                  a.nav-link(href='https://twitter.com/templateflip', title='Twitter')
